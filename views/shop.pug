extends layouts/main-layouts.pug

block styles 
    link(rel="stylesheet", href="/css/product.css")
   
block content 
    .main
        if prods.length>0
            .grid 
                each product in prods 
                    article.card__header 
                        h1.product__title #{product.title}
                    .card__image    
                        img(src="data:image/jpeg;base64,",alt="book")
                    div.card__content
                        h2.product__price $10
                        p.product__discription very intersting 
                    .card__actions 
                        button.btn add to cart

        else 
            h1 No products
                             


